# Working with folders
alias ls='ls -lisah'
alias rm='trash'

# Git
alias ga='git add -p'
alias gc='git commit'
alias gr='git checkout -p --'

# Vagrant
alias vu='sudo echo '' && vagrant up'
# vagrant sometimes needs admin permissions. With this you can vu & get a coffee
alias vh='vagrant halt'
alias vs='vagrant ssh'

# Other
alias ip='ifconfig | grep "inet "'
alias wifi='wifi-password'
